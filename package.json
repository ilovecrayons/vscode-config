{
	"name": "small-kitten-extension",
	"displayName": "small kitten extension",
	"description": "meow",
	"version": "1.0.0",
	"publisher": "ilovecrayons",
	"author": "ilovecrayons",
	"license": "MIT",
	"homepage": "https://github.com/ilovecrayons/vscode-config",
	"bugs": {
		"url": "https://github.com/ilovecrayons/vscode-config/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ilovecrayons/vscode-config.git"
	},
	"keywords": [
		"typing",
		"effect",
		"animation",
		"glow",
		"synthwave",
		"theme",
		"visual"
	],
	"engines": {
		"vscode": "^1.96.0"
	},
	"categories": [
		"Other"
	],
	"main": "out/extension.js",
	"contributes": {
		"themes": [
			{
				"label": "small kitten theme",
				"uiTheme": "vs-dark",
				"path": "./themes/small-kitten-theme.json"
			}
		],
		"commands": [
			{
				"command": "small-kitten-extension.enableMeowing",
				"title": "meowing: enable",
				"category": "meowing effect"
			},
			{
				"command": "small-kitten-extension.disableMeowing",
				"title": "meowing: disable",
				"category": "meowing effect"
			}
		],
		"configuration": {
			"title": "small kitten extension",
			"properties": {
			}
		}
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/json-schema": "^7.0.15",
		"@types/minimatch": "^5.1.2",
		"@types/mocha": "^10.0.1",
		"@types/node": "^16.18.124",
		"@types/semver": "^7.5.6",
		"@types/vscode": "^1.96.0",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"eslint": "^8.45.0",
		"mocha": "^10.2.0",
		"typescript": "^5.7.3"
	},
	"extensionDependencies": [
		"be5invis.vscode-custom-css"
	],
	"icon": "images/neko.jpg",
	"dependencies": {
		"@eslint/eslintrc": "^2.1.4",
		"eslint": "^8.57.0",
		"glob": "^10.3.10",
		"lru-cache": "^7.14.1",
		"rimraf": "^5.0.5"
	},
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true,
			"description": "This extension can modify user settings"
		}
	}
}